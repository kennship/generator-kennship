{
  "name": "generator-kennship",
  "version": "0.0.0",
  "description": "Common tasks for new projects",
  "keywords": [
    "babel",
    "code-style",
    "eslint",
    "generator",
    "kennship",
    "pack",
    "pika",
    "pikapkg",
    "semantic-release",
    "yeoman-generator"
  ],
  "homepage": "",
  "bugs": "https://github.com/kennship/generator-kennship/issues",
  "repository": "kennship/generator-kennship",
  "license": "MIT",
  "author": {
    "name": "Ryan Kennedy",
    "email": "ryaninvents@users.noreply.github.com",
    "url": "https://ryaninvents.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "pretest": "npm run -s lint",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "xo",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/**",
          "*.test.js"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "func-names": "off"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "hosted-git-info": "^2.7.1",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.13.0",
    "keytar": "^4.4.1",
    "lodash": "^4.17.11",
    "mdast-util-definitions": "^1.2.3",
    "remark": "^10.0.1",
    "sort-package-json": "^1.22.0",
    "unist-util-visit": "^1.4.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "semantic-release": "^15.13.3",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm"
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
