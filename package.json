{
  "name": "generator-kennship",
  "version": "0.0.0",
  "description": "Common tasks for new projects",
  "keywords": [
    "babel",
    "code-style",
    "eslint",
    "generator",
    "kennship",
    "pack",
    "pika",
    "pikapkg",
    "semantic-release",
    "yeoman-generator"
  ],
  "homepage": "",
  "repository": "ryaninvents/generator-kennship",
  "license": "MIT",
  "author": {
    "name": "Ryan Kennedy",
    "email": "ryaninvents@users.noreply.github.com",
    "url": "https://ryaninvents.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run -s lint",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "xo",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/**",
          "*.test.js"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "func-names": "off"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.11",
    "sort-package-json": "^1.22.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  }
}
